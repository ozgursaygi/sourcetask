        $(function () {

            var menuId = '@BaseDB.SessionContext.Current.ActiveUser.MenuId';
            var actionName = '@ViewContext.RouteData.GetRequiredString("action")';
            
            if (menuId != undefined && menuId != null && menuId != "" && actionName != "ListPage") {
                var LIs = $("#" + menuId);
                if (LIs != undefined && LIs != null) {
                    LIs.closest('ul').addClass('open');
                    LIs.closest('ul').closest('li').addClass('active');
                    LIs.closest('ul').show();
                    LIs.addClass('active');
                }
            }

            if ('@System.Web.HttpContext.Current.Request.HttpMethod' != 'GET' && '@BaseDB.SessionContext.Current.ActiveUser.FromUpdateButton'!='0') {
                $('.alert-info').show();
            }

            if ('@ViewBag.Success' == 'false' || '@ViewBag.Success' == 'False') {
                $('#message_div').removeClass("alert-info");
                $('#message_div').addClass("alert-danger");
            }
            else if ('@ViewBag.Success' == 'true' || '@ViewBag.Success' == 'True') {
                $('.validation-summary-valid').hide();
                $('#message_div').removeClass("alert-info");
                $('#message_div').addClass("alert-success");
            }

           

        });

        $(document).mouseup(function (e) {
            var container = $("#NotificationDropDown");
            
            if (!container.is(e.target) // if the target of the click isn't the container...
                && container.has(e.target).length === 0) // ... nor a descendant of the container
            {
                container.hide();
            }
        });

        function ShowNotificationList() {
            urlAPI = "/Home/NotificationList?userId=@BaseDB.SessionContext.Current.ActiveUser.UserUid"
            getAjax(ajaxType.GET, urlAPI, ajaxDataType.HTML, ajaxAsync.TRUE, ajaxShowProgress.TRUE, callbackShowNotification, "");
        }

        function callbackShowNotification(resultHTML) {
            $("#divNotificationList").html(resultHTML);
            $("#NotificationDropDown").show();
        }

      
    </script>


</body>

</html>

