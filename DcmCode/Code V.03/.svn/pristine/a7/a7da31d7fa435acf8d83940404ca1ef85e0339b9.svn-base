@model Dcm.Models.Tree


@foreach (var node in Model.RootNode.Children)
{
    if (node.ParentMenuId == 0)
    {
        if (Dcm.Source.GlobalHelper.HasAuthorizedChild(node.MenuId.ToString()))
        { 
        <li id="@node.MenuId"><a href="@node.ObjectName" style="cursor:pointer"><i class="fa @node.IconClass"></i> <span>@node.Name</span></a>@Html.DisplayFor(x => node)</li>
        }
    }

}